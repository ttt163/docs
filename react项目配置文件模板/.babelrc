{
  "presets": [
    "react",
    "es2015",
    'stage-0'
  ],
  plugins: [
    "transform-runtime",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib",
        "style": "css"
      }
    ]
  ],
  //打包时去掉下面文件
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}